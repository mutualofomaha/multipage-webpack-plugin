{
  "name": "multipage-webpack-plugin",
  "version": "1.0.0",
  "description": "Currently to architecht a webpack configuration for multi page web applications, there are many requirements for managing all assets and entry points.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "BABEL_ENV=production babel --out-dir=dist src/plugin.js",
    "build:examples": "webpack --progress --colors --config ./examples",
    "build:debug": "node --debug-brk --inspect ./node_modules/webpack/bin/webpack.js --config ./examples",
    "test": "npm run build:examples && nyc ava",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutualofomaha/multipage-webpack-plugin.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mutualofomaha/multipage-webpack-plugin/issues"
  },
  "homepage": "https://github.com/mutualofomaha/multipage-webpack-plugin#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.16.3",
    "bluebird": "^3.4.6",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.4.1",
    "hard-source-webpack-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.22.0",
    "memory-fs": "^0.4.1",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "webpack": "beta"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/*.test.js",
      "!**/not-this-file.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "require": [
      "babel-register"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "ava"
    ],
    "extends": "plugin:ava/recommended"
  }
}
